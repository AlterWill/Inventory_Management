package com.app;
import javax.swing.*;
import com.formdev.flatlaf.FlatDarkLaf;
import java.awt.*;


class Counter{
 private int count;
  Counter(int initial){
	count = initial;
 }
 public int getCount(){
	 return count;
 }

}

public class Main {
  		
  public static void main(String[] args) {
		 
// enabling flatlaf for UI 
		  try {
		  UIManager.setLookAndFeel(new FlatDarkLaf());
		  } catch (Exception e) {
		      e.printStackTrace();	
		  }



		  
		}
}
